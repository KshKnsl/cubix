{
  "name": "cubix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile-engine": "node scripts/compile-engine.js",
    "postinstall": "npm run compile-engine",
    "server": "nodemon server/server.js",
    "client": "next dev --turbopack",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "build:full": "npm run build && cross-env NODE_ENV=production node server/server.js",
    "start:prod": "cross-env NODE_ENV=production node server/production.js",
    "deploy": "next build && touch out/.nojekyll"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.7",
    "@react-three/fiber": "^9.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lucide-react": "^0.476.0",
    "motion": "^12.4.7",
    "multer": "^1.4.5-lts.1",
    "next": "15.1.7",
    "nodemon": "^3.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
